  ensure_installed = { "lua", "python", "go", "ruby", "php", "tsx", "css", "html", "javascript", "typescript" },
  highlight = { enable = true, additional_vim_regex_highlighting = false },
  indent = { enable = true },
  autotag = { enable = true },
  fold = { enable = true },
}
